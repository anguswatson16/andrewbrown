        <li role="presentation" class="active">
          <a href="#home" id="home-tab" class="tab-text" role="tab" data-toggle="tab" aria-controls="home" aria-expanded="true">
            <span class="text outstanding-text">Home</span>
          </a>
        </li>
        <li role="presentation" class="next">
          <a href="about" role="tab" class="tab-text" id="about-tab" data-toggle="tab" aria-controls="about">
