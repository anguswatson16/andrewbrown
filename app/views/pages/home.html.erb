            <span class="text outstanding-text">Publications</span>
          </a>
        </li>
        <li role="presentation" class="next">
          <a href="#cv" role="tab" class="tab-text" id="cv-tab" data-toggle="tab" aria-controls="cv">
            <span class="text outstanding-text">CV</span>
          </a>
        </li>
      </ul>
      <div id="myTabContent" class="tab-content">
        <div role="tabpanel" class="tab-pane fade in active" id="home" aria-labelledby="home-tab">

        </div>
        <div role="tabpanel" class="tab-pane fade" id="about" aria-labelledby="about-tab">

        </div>
        <div role="tabpanel" class="tab-pane fade" id="news" aria-labelledby="news-tab">

        </div>
        <div role="tabpanel" class="tab-pane fade" id="projects" aria-labelledby="projects-tab">

        </div>
        <div role="tabpanel" class="tab-pane fade" id="publications" aria-labelledby="publications-tab">

        </div>
        <div role="tabpanel" class="tab-pane fade" id="cv" aria-labelledby="cv-tab">

        </div>
      </div>
    </div>
  </div>
</div>
