<%= form_for(publication) do |p| %>
  <%= p.label :name %>
  <%= p.text_field :name %>
  <%= p.label :description %>
  <%= p.text_field :description %>
  <%= p.label :website %>
  <%= p.text_field :website %>
  <%= p.label :collaborators %>
  <%= p.text_field :collaborators %>
  <%= p.label :journal %>
  <%= p.text_field :journal %>
  <%= p.submit %>
<% end %>
